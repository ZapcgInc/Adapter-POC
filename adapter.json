{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "saiteja50/hopperadapterscala",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 7001, "hostPort": 0, "protocol": "tcp" }
	
      ]
    }
  },
  "cmd": "sbt run",
  "id": "hopperadapterscala",
  "instances": 1,
  "cpus": 0.1,
  "mem": 1024,
  "healthChecks": [
      {
        "protocol": "TCP",
        "portIndex": 0,
        "path": "/",
        "intervalSeconds": 20,
        "maxConsecutiveFailures": 3
      }

    ]
}
